{
  "name": "@enterprise/app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "GENERATE_SOURCEMAP=true REACT_APP_VERSION=$npm_package_version react-app-rewired build",
    "format": "prettier --write \"**/*.{js,ts,tsx}\"",
    "generate-types": "node --experimental-specifier-resolution=node --loader ts-node/esm ../../scripts/generate-types.ts enterprise",
    "lint": "eslint \"./src/{**/*,*}.{ts,tsx}\"",
    "start": "REACT_APP_VERSION=$npm_package_version PORT=4001 react-app-rewired start",
    "test": "react-app-rewired test --passWithNoTests"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "./.eslintrc.js"
    ]
  },
  "dependencies": {
    "@floating-ui/dom": "^1.2.9",
    "@floating-ui/react": "^0.24.2",
    "@hookform/resolvers": "^2.9.7",
    "@sentry/integrations": "^7.52.1",
    "@sentry/react": "^7.52.1",
    "@terra-money/feather.js": "^1.0.4",
    "@terra-money/log-finder-ruleset": "^3.0.2",
    "@terra-money/terra-station-mobile": "1.0.8",
    "@terra-money/wallet-kit": "1.0.11",
    "@terra.kitchen/utils": "^1.0.7",
    "ace-builds": "^1.23.4",
    "big.js": "^6.2.1",
    "classnames": "^2.3.1",
    "date-fns": "^2.29.3",
    "focus-trap-react": "^9.0.2",
    "json-schema-to-typescript": "^11.0.2",
    "lodash": "^4.17.21",
    "notistack": "^3.0.1",
    "react": "^18.2.0",
    "react-ace": "^10.1.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.2",
    "react-hook-form": "^7.34.2",
    "react-query": "^3.39.2",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "react-transition-group": "^4.4.5",
    "react-use": "^17.4.0",
    "redux-thunk": "^2.4.1",
    "rxjs": "^7.8.0",
    "sass": "^1.54.5",
    "styled-components": "^5.3.5",
    "typescript": "^4.9.4",
    "usehooks-ts": "^2.6.0",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/big.js": "^6.1.5",
    "@types/jest": "^28.1.8",
    "@types/node": "^16.11.56",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-virtualized": "^9.21.21",
    "@types/react-window": "^1.8.5",
    "@types/styled-components": "^5.1.25",
    "crypto-browserify": "^3.12.0",
    "customize-cra": "^1.0.0",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "https-browserify": "^1.0.0",
    "husky": "^8.0.3",
    "jest-watch-typeahead": "2.1.1",
    "lint-staged": "^13.2.2",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "prettier": "^2.8.8",
    "react-app-rewired": "^2.2.1",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "ts-node": "^10.9.1",
    "url": "^0.11.0"
  },
  "generate-types": {
    "contracts": [
      "enterprise",
      "enterprise-factory",
      "funds-distributor"
    ],
    "output": "src/types/contracts"
  },
  "importSort": {
    ".js, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  }
}
